{
    "name": "AJ's New Tab",
    "version": "1.0",
    "manifest_version": 2,
    "description": "AJ's Clean New Tab",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
      "history",
      "sessions",
      "tabs",
      "file:///*",
      "storage"
    ],
    "icons": {
           "16": "images/icon16.png",
           "48": "images/icon48.png"}
}
